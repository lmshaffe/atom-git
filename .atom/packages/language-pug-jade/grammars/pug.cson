  }
  {
    'begin': '^(\\s*):(markdown)(?=\\(|$)$'
    'beginCaptures':
      '2':
        'name': 'constant.language.name.markdown.filter.pug'
    'end': '^(?!(\\1\\s)|\\s*$)'
    'name': 'text.markdown.filter.pug'
    'patterns': [
      {
        'include': '#filter_args'
      }
      {
        'include': 'text.html.markdown'
      }
    ]
  }
  {
    'begin': '^(\\s*):(sass)(?=\\(|$)$'
    'beginCaptures':
      '2':
        'name': 'constant.language.name.sass.filter.pug'
    'end': '^(?!(\\1\\s)|\\s*$)'
    'name': 'source.sass.filter.pug'
    'patterns': [
      {
        'include': '#filter_args'
      }
      {
        'include': 'source.sass'
      }
    ]
  }
  {
    'begin': '^(\\s*):(less)(?=\\(|$)$'
    'beginCaptures':
