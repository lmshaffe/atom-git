        'name': 'constant.language.name.less.filter.pug'
    'end': '^(?!(\\1\\s)|\\s*$)'
    'name': 'source.less.filter.pug'
    'patterns': [
      {
        'include': '#filter_args'
      }
      {
        'include': 'source.less'
      }
    ]
  }
  {
    'begin': '^(\\s*):(stylus)(?=\\(|$)$'
    'beginCaptures':
      '2':
        'name': 'constant.language.name.stylus.filter.pug'
    'end': '^(?!(\\1\\s)|\\s*$)'
    'name': 'source.stylus.filter.pug'
    'patterns': [
      {
        'include': '#filter_args'
      }
      {
        'include': 'source.stylus'
